@page "/notice-push"

<div class="container">
    <div class="block__style">
        <div class="settings_tabs top__title flex">
            <div class="settings__tab-main flex" id="notice__push-tabs">
                <button class="settings__tab tab__active" type="button">Полученные</button>
                <button class="settings__tab" type="button">Отправленные</button>
            </div>
        </div>
        <ul class="notice__content">
            <li class="main__notice new-main__notice top__title somewhere__user">
                <div class="flex justify-content">
                    <div class="main__notice-user flex">
                        <div class="main__notice-image people__image">
                            <img class="people__img" src="./img/photo/img7.png" alt="avatar">
                        </div>
                        <div class="main__notice-text__description">
                            <p class="main__notice-text new__notice text">
                                Владимир приглашает вас на
                                <a href="#">
                                    «Матрица»
                                </a>
                            </p>
                            <div class="notice__push-btns flex">
                                <button class="invitation__btn small__text" type="button">
                                    Принять
                                </button>
                                <button class="invitation__btn invitation__btn-cancel small__text" type="button">
                                    Отменить
                                </button>
                            </div>
                        </div>
                    </div>
                    <span class="timing">
                        18
                        <span>
                            минут назад
                        </span>
                    </span>
                </div>
            </li>
